<h1>Binevenidos</h1>
<section>
    <h1>hola: {{user.first_name}}</h1>
    <h2>email:{{user.email}}</h2>
</section>
<form id="logout">
    <button type="submit"> SALIR</button>
</form>
<a href="/carritos" class="btn">Ver Carrito</a>

{{#each productos}}
    <p>id: {{this._id}}</p>
    <p>titulo: {{this.title}}</p>
    <p>description: {{this.description}}</p> 
    <p>codigo: {{this.code}}</p> 
    <p>precio: {{this.price}}</p>
    <p>estatus: {{this.status}}</p>  
    <p>stock: {{this.stock}}</p> 
    <p>categoria: {{this.category}}</p> 
    <button class="agregar-carrito" data-producto-id="{{this._id}}">agregar</button>
    <hr>
{{/each}}
<script src="./js/agregar-carrito.js"></script>
<script src="./js/logout.js"></script> 



